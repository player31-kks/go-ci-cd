{
    "executionRoleArn": "arn:aws:iam::141145281548:role/ecsTaskExecutionRole",
    "taskRoleArn": "",
    "containerDefinitions": [
      {
        "image": "test",
        "logConfiguration": {
          "logDriver": "awslogs",
          "options": {
            "awslogs-group": "/ecs/go",
            "awslogs-region": "ap-northeast-2",
            "awslogs-stream-prefix": "ecs"
          }
        },
        "cpu": 0,
        "portMappings": [
          {
            "hostPort": 3000,
            "protocol": "tcp",
            "containerPort": 3000
          }
        ],
        "ulimits": [
          {
            "name": "nofile",
            "softLimit": 65535,
            "hardLimit": 65535
          }
        ],
        "name": "go",
        "essential": true
      }
    ],
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory" : "512",
    "family": "go"
  }